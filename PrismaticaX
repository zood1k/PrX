    Title = "Spinbot",
    Desc = "Enable spinbot",
    Value = false,
    Callback = function(state)
        SpinbotEnabled = state
        if state then
            Spinbot:EnableSpinbot()
        else
            Spinbot:DisableSpinbot()
        end
    end
})
