-- Clear button
ClearVisualSection:Button({
    Title = "Clear",
    Desc = "Clear all visuals",
    Icon = "trash-2",
    Callback = function()
        EffectManager:ClearVisual()
        -- Disable visual toggles
        for name, toggle in pairs(toggles) do
            if name:match("ChinaHat") or name:match("Trails") or name:match("JumpEffect") or name:match("Particles") or name:match("NeonGlow") or name:match("StarTrail") or name:match("FireAura") or name:match("SmokeTrail") or name:match("SparkleBody") or name:match("ColorShift") then
                toggle:Set(false)
            end
        end
    end
})

-- Player modification toggles and sliders
local speedToggle = PlayerModsSection:Toggle({
    Title = "Speed",
    Desc = "Enable speed hack",
    Value = false,
    Callback = function(state)
        if state then
            if not speedConnection then
                speedConnection = RunService.Heartbeat:Connect(function()
                    local char = LocalPlayer.Character
                    local humanoid = char and char:FindFirstChild("Humanoid")
                    if humanoid then
                        humanoid.WalkSpeed = CurrentWalkSpeed
                    end
                end)
            end
        else
            if speedConnection then
                speedConnection:Disconnect()
                speedConnection = nil
            end
            local char = LocalPlayer.Character
            local humanoid = char and char:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = OriginalWalkSpeed
            end
        end
    end
})
toggles.Speed = speedToggle

PlayerModsSection:Slider({
    Title = "Speed Changer",
    Desc = "Set walk speed (16-100)",
    Value = { Min = 16, Max = 100, Default = 16 },
    Callback = function(value)
        CurrentWalkSpeed = value
        if speedToggle.Value then
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = CurrentWalkSpeed
            end
        end
    end
})

local jumpToggle = PlayerModsSection:Toggle({
    Title = "Jump",
    Desc = "Enable jump power hack",
    Value = false,
    Callback = function(state)
        if state then
            if not jumpConnection then
                jumpConnection = RunService.Heartbeat:Connect(function()
                    local char = LocalPlayer.Character
                    local humanoid = char and char:FindFirstChild("Humanoid")
                    if humanoid then
                        humanoid.UseJumpPower = true
                        humanoid.JumpPower = CurrentJumpPower
                    end
                end)
            end
        else
            if jumpConnection then
                jumpConnection:Disconnect()
                jumpConnection = nil
            end
            local char = LocalPlayer.Character
            local humanoid = char and char:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.UseJumpPower = OriginalUseJumpPower
                if OriginalUseJumpPower then
                    humanoid.JumpPower = OriginalJumpPower
                else
                    humanoid.JumpHeight = OriginalJumpHeight
                end
            end
        end
    end
})
toggles.Jump = jumpToggle

PlayerModsSection:Slider({
    Title = "Jump Changer",
    Desc = "Set jump power (50-100)",
    Value = { Min = 50, Max = 100, Default = 50 },
    Callback = function(value)
        CurrentJumpPower = value
        if jumpToggle.Value then
            local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.JumpPower = CurrentJumpPower
            end
        end
    end
})

local infiniteJumpToggle = PlayerModsSection:Toggle({
    Title = "Infinite Jump",
    Desc = "Enable infinite jump",
    Value = false,
    Callback = function(state)
        InfiniteJumpEnabled = state
    end
})
toggles.InfiniteJump = infiniteJumpToggle

local noclipToggle = PlayerModsSection:Toggle({
    Title = "Noclip",
    Desc = "Enable noclip",
    Value = false,
    Callback = function(state)
        NoclipEnabled = state
        if state then
            if not noclipConnection then
                noclipConnection = RunService.Stepped:Connect(function()
                    if NoclipEnabled and LocalPlayer.Character then
                        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                            if part:IsA("BasePart") and part.CanCollide then
                                part.CanCollide = false
                            end
                        end
                    end
                end)
            end
        else
            if noclipConnection then
                noclipConnection:Disconnect()
                noclipConnection = nil
            end
            if LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
        end
    end
})
toggles.Noclip = noclipToggle

local flyToggle = PlayerModsSection:Toggle({
    Title = "Fly",
    Desc = "Enable fly (WASD to move)",
    Value = false,
    Callback = function(state)
        FlyEnabled = state
        if state then
            if not flyConnection then
                flyConnection = RunService.Heartbeat:Connect(function()
                    if not FlyEnabled then return end
                    local char = LocalPlayer.Character
                    if not char then return end
                    local root = char:FindFirstChild("HumanoidRootPart")
                    if not root then return end
                    if not flyBodyVelocity or not flyBodyVelocity.Parent then
                        flyBodyVelocity = Instance.new("BodyVelocity")
                        flyBodyVelocity.Velocity = Vector3.new(0, 0, 0)
                        flyBodyVelocity.MaxForce = Vector3.new(1e9, 1e9, 1e9)
                        flyBodyVelocity.Parent = root
                    end
                    local camera = workspace.CurrentCamera
                    local direction = Vector3.new()
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        direction = direction + camera.CFrame.LookVector * FlySpeed
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        direction = direction - camera.CFrame.LookVector * FlySpeed
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        direction = direction - camera.CFrame.RightVector * FlySpeed
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        direction = direction + camera.CFrame.RightVector * FlySpeed
                    end
                    flyBodyVelocity.Velocity = direction
                end)
            end
        else
            if flyConnection then
                flyConnection:Disconnect()
                flyConnection = nil
            end
            if flyBodyVelocity then
                pcall(function() flyBodyVelocity:Destroy() end)
                flyBodyVelocity = nil
            end
        end
    end
})
toggles.Fly = flyToggle

PlayerModsSection:Slider({
    Title = "Fly Speed",
    Desc = "Set fly speed (1-100)",
    Value = { Min = 1, Max = 100, Default = 50 },
    Callback = function(value)
        FlySpeed = value
    end
})

-- Infinite Jump handler
UserInputService.JumpRequest:Connect(function()
    if InfiniteJumpEnabled and LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            pcall(function() humanoid:ChangeState("Jumping") end)
        end
    end
end)

-- Handle window close to cleanup all features
Window:OnClose(function()
    CleanupAll()
end)

-- Cleanup on player leaving
Players.PlayerRemoving:Connect(function(leavingPlayer)
    if leavingPlayer == LocalPlayer then
        CleanupAll()
    else
        if lockedTarget == leavingPlayer then
            lockedTarget = nil
        end
    end
end)
