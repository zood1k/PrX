    Title = "Aimbot Fov",
    Desc = "Set aimbot FOV (10-800)",
    Value = { Min = 10, Max = 800, Default = 100 },
    Callback = function(value)
        aimbotFOV = value
        if fovCircle and fovCircle.Parent then
            fovCircle.Size = UDim2.new(0, aimbotFOV * 2, 0, aimbotFOV * 2)
        end
    end
})

